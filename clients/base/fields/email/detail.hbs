{{#each value}}
    {{#if hasAnchor}}
        <a rel="tooltip"{{#if ../../dir}} dir="{{../../../dir}}"{{/if}} title="{{email_address}}{{#notEq flagLabel ''}} ({{flagLabel}}){{/notEq}}" data-placement="bottom" class="ellipsis_inline{{#if opt_out}} opt-out{{/if}}" href="javascript:void(0);" data-action="email" data-email-address-id="{{email_address_id}}" data-email-to="{{email_address}}" data-email-opt-out="{{opt_out}}" track="click:email_address">{{email_address}}</a>
    {{else}}
        {{#if invalid_email}}
            <del rel="tooltip"{{#if ../../../dir}} dir="{{../../../../dir}}"{{/if}} title="{{email_address}} ({{flagLabel}})" data-placement="bottom" class="ellipsis_inline{{#if opt_out}} opt-out{{/if}}">{{email_address}}</del>
        {{else}}
            <span rel="tooltip"{{#if ../../../dir}} dir="{{../../../../dir}}"{{/if}} title="{{email_address}}{{#notEq flagLabel ''}} ({{flagLabel}}){{/notEq}}" data-placement="bottom" class="ellipsis_inline{{#if opt_out}} opt-out{{/if}}">{{email_address}}</span>
        {{/if}}
    {{/if}}
{{/each}}
